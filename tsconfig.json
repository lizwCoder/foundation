{
  "compilerOptions": {
    "outDir": "./built/",
    "sourceMap": true,
    "strict": true,
    "noImplicitReturns": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "experimentalDecorators":true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/main/resources/static/src/*"
      ]
    }
  },
  "include": [
    "./src/main/resources/static/**/*"
  ]
}